USE DATABASE master;
USE SCHEMA dbo;
 @departments  = 
     SELECT * FROM 
         (VALUES
             (31,    "Sales"),
             (33,    "Engineering"), 
             (34,    "Clerical"),
             (35,    "Marketing")
         ) AS 
               D( DepID, DepName );

 OUTPUT @departments
     TO "/tmp/departments2.csv"
 USING Outputters.Csv();
